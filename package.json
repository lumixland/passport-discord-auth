{
  "name": "passport-discord-auth",
  "version": "1.0.0",
  "description": "An updated passport authentication strategy for Discord. Lumix forked.",
  "keywords": [
    "discord.js",
    "discord",
    "utils",
    "slekup",
    "nodejs",
    "utility"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lumixland/passport-discord-auth"
  },
  "license": "CC-BY-NC-SA-4.0",
  "authors": [
    "Slekup",
    "Lumix OSS Team"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "nodemon --watch src/** --ext ts,json --exec \"pnpm build\"",
    "commit": "commit",
    "dev": "tsup --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "dependencies": {
    "builder-validation": "^0.0.3",
    "passport": "^0.7.0",
    "passport-oauth2": "^1.8.0"
  },
  "devDependencies": {
    "@types/passport": "^1.0.16",
    "@types/passport-oauth2": "^1.4.17",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  }
}
